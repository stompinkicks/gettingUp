<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">

  <% @users.each do |user| %>
    <li>      
    <%= user.name %>
    <%= image_tag user.avatar.url %> 
     <% user.patterns.each do |pattern| %>
     <%= image_tag pattern.image (:medium) %>
    </li>
  <% end %>
<% end %>

</ul>